{
  "name": "vue-admin",
  "description": "Vue Admin Template.",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "server": "vite preview",
    "lint": "npm run prettier && npm run eslint",
    "eslint": "eslint ./ --fix --ext .ts,.tsx,.js,.jsx,.vue",
    "prettier": "prettier --write ./**/*.{ts,tsx,js,jsx,vue,html,css,scss,less}",
    "update-x": "npm i js-xhttp js-xxx js-xcall",
    "clean": "xcmd clean",
    "time": "xcmd update-time"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run time"
    }
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "ant-design-vue": "^3.2.3",
    "js-xcall": "^1.1.1",
    "js-xhttp": "^1.4.9",
    "js-xxx": "^1.2.2",
    "store": "^2.0.12",
    "vue": "^3.2.25",
    "vue-router": "^4.0.15",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.31",
    "@types/store": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^8.0.1",
    "js-xcmd": "^1.2.1",
    "prettier": "^2.6.2",
    "sass": "^1.51.0",
    "typescript": "^4.5.4",
    "vite": "^2.9.7",
    "vue-tsc": "^0.34.7"
  },
  "time": "3413551612052022"
}